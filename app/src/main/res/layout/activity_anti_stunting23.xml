<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.stuntcare.AntiStunting2_3">

    <ProgressBar
        android:id="@+id/progressBar5"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="84dp"
        android:layout_height="6dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="75dp"
        android:background="@color/text_color_register"
        android:progressTint="@color/text_color_register"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/progressBar6"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="84dp"
        android:layout_height="6dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="75dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/progressBar7"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="84dp"
        android:layout_height="6dp"
        android:layout_marginStart="200dp"
        android:layout_marginTop="75dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/progressBar8"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="84dp"
        android:layout_height="6dp"
        android:layout_marginStart="292dp"
        android:layout_marginTop="75dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="97dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_arrowback" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="282dp"
        android:layout_height="24dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="99dp"
        android:text="@string/prePregnancyWeight"
        android:textColor="@color/text_color_register"
        android:fontFamily="@font/poppins_bold"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="328dp"
        android:layout_height="60dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="153dp"
        android:text="@string/inputyourprepregnancy"
        android:textColor="@color/text_color_register"
        android:fontFamily="@font/poppins_rg"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextWeight"
        android:layout_width="73dp"
        android:layout_height="64dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="245dp"
        android:background="@drawable/datebutton"
        android:backgroundTint="@color/kotakBerat"
        android:hint="0"
        android:inputType="numberDecimal"
        android:padding="8dp"
        android:paddingStart="8dp"
        android:paddingEnd="8dp"
        android:textAlignment="center"
        android:textColor="@color/text_color_register"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="115dp"
        android:layout_height="35dp"
        android:layout_marginStart="139dp"
        android:layout_marginTop="72dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/kilogram"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editTextWeight" />

    <Button
        android:id="@+id/button6"
        android:layout_width="361dp"
        android:layout_height="46dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="638dp"
        android:text="@string/next"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>